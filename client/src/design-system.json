      "500": "#3b82f6",
      "600": "#2563eb",
      "700": "#1d4ed8",
      "800": "#1e40af",
      "900": "#1e3a8a"
    },
    "secondary": {
      "50": "#f8fafc",
      "100": "#f1f5f9",
      "200": "#e2e8f0",
      "300": "#cbd5e1",
      "400": "#94a3b8",
      "500": "#64748b",
      "600": "#475569",
      "700": "#334155",
      "800": "#1e293b",
      "900": "#0f172a"
    },
    "accent": {
      "50": "#fdf4ff",
      "100": "#fae8ff",
      "200": "#f5d0fe",
      "300": "#f0abfc",
      "400": "#e879f9",
      "500": "#d946ef",
      "600": "#c026d3",
      "700": "#a21caf",
      "800": "#86198f",
      "900": "#701a75"
    },
    "success": "#10b981",
    "warning": "#f59e0b",
    "error": "#ef4444",
    "white": "#ffffff",
    "black": "#000000"
  },
  "typography": {
    "fontFamily": {
      "sans": ["Inter", "system-ui", "sans-serif"],
      "mono": ["JetBrains Mono", "monospace"]
    },
    "fontSize": {
      "xs": "0.75rem",
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.5rem",
      "3xl": "1.875rem",
      "4xl": "2.25rem",
      "5xl": "3rem",
      "6xl": "3.75rem"
    },
    "fontWeight": {
      "light": "300",
      "normal": "400",
      "medium": "500",
      "semibold": "600",
      "bold": "700",
      "extrabold": "800"
    },
    "lineHeight": {
      "tight": "1.25",
      "normal": "1.5",
      "relaxed": "1.75"
    }
  },
  "spacing": {
    "xs": "0.25rem",
    "sm": "0.5rem",
    "md": "1rem",
    "lg": "1.5rem",
    "xl": "2rem",
    "2xl": "3rem",
    "3xl": "4rem",
    "4xl": "6rem"
  },
  "borderRadius": {
    "sm": "0.25rem",
    "md": "0.375rem",
    "lg": "0.5rem",
    "xl": "0.75rem",
    "2xl": "1rem",
    "full": "9999px"
  },
  "shadows": {
    "sm": "0 1px 2px 0 rgb(0 0 0 / 0.05)",
    "md": "0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1)",
    "lg": "0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1)",
    "xl": "0 20px 25px -5px rgb(0 0 0 / 0.1), 0 8px 10px -6px rgb(0 0 0 / 0.1)"
  },
  "components": {
    "navigation": {
      "backgroundColor": "#ffffff",
      "borderBottom": "1px solid #e2e8f0",
      "padding": "1rem 0",
      "logo": {
        "fontSize": "1.25rem",
        "fontWeight": "700",
        "color": "#2563eb"
      },
      "navItem": {
        "padding": "0.5rem 1rem",
        "borderRadius": "0.375rem",
        "transition": "all 0.2s ease",
        "hover": {
          "backgroundColor": "#eff6ff",
          "color": "#1d4ed8"
        },
        "active": {
          "backgroundColor": "#dbeafe",
          "color": "#1e40af"
        }
      }
    },
    "button": {
      "primary": {
        "backgroundColor": "#2563eb",
        "color": "#ffffff",
        "padding": "0.5rem 1.5rem",
        "borderRadius": "0.375rem",
        "fontWeight": "500",
        "border": "none",
        "cursor": "pointer",
        "transition": "all 0.2s ease",
        "hover": {
          "backgroundColor": "#1d4ed8",
          "transform": "translateY(-1px)",
          "boxShadow": "0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1)"
        }
      },
      "secondary": {
        "backgroundColor": "transparent",
        "color": "#2563eb",
        "padding": "0.5rem 1.5rem",
        "borderRadius": "0.375rem",
        "fontWeight": "500",
        "border": "2px solid #2563eb",
        "cursor": "pointer",
        "transition": "all 0.2s ease",
        "hover": {
          "backgroundColor": "#2563eb",
          "color": "#ffffff"
        }
      }
    },
    "card": {
      "backgroundColor": "#ffffff",
      "borderRadius": "0.5rem",
      "padding": "2rem",
      "boxShadow": "0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1)",
      "border": "1px solid #e2e8f0"
    },
    "hero": {
      "backgroundColor": "linear-gradient(135deg, #eff6ff 0%, #fdf4ff 100%)",
      "padding": "6rem 0",
      "textAlign": "center"
    }
  }
}
